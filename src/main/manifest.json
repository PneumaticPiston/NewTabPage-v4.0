{
    "manifest_version": 3,
    "name": "Custom New Tab Page",
    "version": "2.0.1",
    "description": "Customizes the new tab page with a personalized layout and extra features.",
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "chrome_url_overrides": {
        
    },
    "options_page": "/pages/ntp/ntp.html#header",
    "icons": {
        "16": "assets/iconx16.png",
        "19": "assets/iconx19.png",
        "38": "assets/iconx38.png",
        "48": "assets/iconx48.png",
        "128": "assets/iconx128.png"
    }
}